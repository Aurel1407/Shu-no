# Variables d'environnement pour les tests
NODE_ENV=test

# Base de données de test (utilisez une DB séparée)
DB_HOST=localhost
DB_PORT=5432
DB_ADMIN_USER=postgres
DB_ADMIN_PASSWORD=postgres
DB_USERNAME=test_user
DB_PASSWORD=test_password
DB_NAME=shu_no_test

# JWT pour tests
JWT_SECRET=test_jwt_secret_key_very_long_and_secure_for_testing_only
