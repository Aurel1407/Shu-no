{
  "name": "shu-no-backend",
  "version": "1.0.0",
  "description": "Backend API for Shu-no g√Æte rental platform",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "nodemon --quiet --exec ts-node src/app.ts",
    "prepare-prod": "node scripts/prepare-production.js",
    "start:prod": "npm run prepare-prod && npm run build && npm start",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "test": "jest",
    "test:setup": "ts-node src/scripts/setup-test-db.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "create-admin": "ts-node src/scripts/create-admin.ts",
    "debug-users": "ts-node src/scripts/debug-users.ts",
    "reset-admin-password": "ts-node src/scripts/reset-admin-password.ts",
    "logs:error": "tail -f logs/error.log",
    "logs:app": "tail -f logs/app.log",
    "audit": "npm audit",
    "audit:fix": "npm audit fix"
  },
  "keywords": [
    "api",
    "express",
    "typeorm",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/dompurify": "^3.0.5",
    "@types/jsdom": "^21.1.7",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dompurify": "^3.2.7",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "jsdom": "^27.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "pg": "^8.11.3",
    "redis": "^5.8.2",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.17",
    "winston": "^3.17.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.8.7",
    "@types/pg": "^8.15.5",
    "@types/redis": "^4.0.10",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^30.1.3",
    "nodemon": "^3.0.1",
    "playwright": "^1.55.0",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
